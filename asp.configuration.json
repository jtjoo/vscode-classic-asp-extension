{
	"comments": {
		"lineComment": "'"
	},
	"brackets": [
		[
		    "(",
		    ")"
		]
	],
	"autoClosingPairs": [
		{
		    "open": "(",
		    "close": ")",
		    "notIn": [
			"string"
		    ]
		},
		{
		    "open": "\"",
		    "close": "\"",
		    "notIn": [
			"string",
			"comment"
		    ]
		}
	],
	"surroundingPairs": [
		[
		    "(",
		    ")"
		],
		[
		    "\"",
		    "\""
		]
	],
	"embeddedLanguages": {
		"script[language='vbscript'][runat='server']": "vbscript",
		"script[language='javascript'][runat='server']": "javascript" // Add support for JavaScript
	}
}
